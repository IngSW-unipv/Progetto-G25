<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="it.unipv.ingsfw.bitebyte.controller.DistributoriAlternativiController" spacing="10" alignment="CENTER" style="-fx-padding: 10;">
    <Label text="Distributori Alternativi" style="-fx-font-weight: bold; -fx-font-size: 14px;"/>
    <TableView fx:id="distributoriTable" prefWidth="550" prefHeight="300">
        <columns>
            <TableColumn fx:id="nomeColumn" text="Nome" prefWidth="150"/>
            <TableColumn fx:id="indirizzoColumn" text="Indirizzo" prefWidth="200"/>
            <TableColumn fx:id="distanzaColumn" text="Distanza (km)" prefWidth="100"/>
            <TableColumn fx:id="azioneColumn" text="Azione" prefWidth="100"/>
        </columns>
    </TableView>
</VBox>
